#include <iostream>
using namespace std;

int main() {
    int num=9;
    
     if(num>0){
     for(int i=0; i<9; i++) {

        for(int j=0; j<=i; j++) {
              if(j==8){
                cout<<'J';
            continue;}
            cout << char('A' + j);

       }
       for(int k=2*(10-i-2); k>=2; k--) {
            cout << " ";
       }
       for(int l=i; l>=0; l--){
            if(l==8)
            continue;
            cout<<char('A' + l);
        }
        cout<<endl;
        }
for(int i=7; i>=0; i--) {
        for(int j=0; j<=i; j++) {
            cout << char('A' + j);
        }
        for(int k=2*(8-i); k>=2; k--) {
            cout << " ";
        }
        for(int l=i; l>=0; l--) {
            cout << char('A' + l);
        }
        cout << endl;
        }}}
